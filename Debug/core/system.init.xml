<System>
    <SystemPackages>
        <Package name="core.intro" minVer="1" mustLoad="true" loadStep="0"></Package>
        <Package name="core.debug" minVer="1" mustLoad="false" loadStep="1"></Package>
        <Package name="core.language" minVer="1" mustLoad="false" loadStep="1"></Package>
        <Package name="core.assets.skys" minVer="1" mustLoad="false" loadStep="1"></Package>
        <Package name="core.sounds" minVer="1" mustLoad="true" loadStep="1"></Package>
        <Package name="core.sounds.music" minVer="1" mustLoad="true" loadStep="1"></Package>
        <Package name="core.ui" minVer="1" mustLoad="true" loadStep="1"></Package>
        <Package name="core.menulevel" minVer="1" mustLoad="true" loadStep="1"></Package>
    </SystemPackages>
    <SystemScenses>
        <Scense>Intro</Scense>
        <Scense>MenuLevel</Scense>
        <Scense>Level</Scense>
        <Scense>GameDebug</Scense>
        <Scense>Editor</Scense>
    </SystemScenses>
    <SystemOptions>
        <CorePackageName>core</CorePackageName>
        <LogToFileEnabled>True</LogToFileEnabled>
        <FirstEnterScense>Intro</FirstEnterScense>
    </SystemOptions>
</System>